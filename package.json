    "svelte-flatpickr": "^2.4.0",
    "svelte-preprocess": "^3.3.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "run-p watch:routify:split watch:rollup",
    "dev:no-split": "run-p watch:routify watch:rollup",
    "watch:rollup": "rollup -c -w",
    "watch:routify": "routify",
    "watch:routify:split": "routify -D",
    "start": "sirv public --single",
    "start:split": "sirv public --single dynamic.html",
    "dev:rollup": "rollup -c -w",
    "dev:livereload": "cross-env LIVERELOAD=1 rollup -c -w",
    "dev:nollup": "cross-env NOLLUP=1 nollup -c --hot --port 5000 --content-base ./public",
    "start": "sirv public"
  }
